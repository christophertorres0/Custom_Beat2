use_bpm 40
use_synth :piano
x=:c4
b=2
2.times do
  play :f2, amp:5
  sleep 0.65
  play :f1, amp:5
  sleep 0.25
end

2.times do
  play :g2, amp:1
  sleep 0.5
  play :g3, amp:1
  sleep 0.5
end

2.times do
  play :f2, amp:5
  sleep 0.25
  play :f1, amp:5
  sleep 0.25
end

2.times do
  play :g2, amp:1
  sleep 0.65
  play :g3, amp:1
  sleep 0.5
end
4.times do
  sample :bd_ada
  sleep 0.5
  sample :bd_gas
  sleep 0.5
  sample :ambi_piano
  play :g2, decay:2
end
